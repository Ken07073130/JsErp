<%@ Page Language="C#" AutoEventWireup="true" CodeFile="jsscrbcx.aspx.cs" Inherits="sclr_jsscrbcx" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" >
<head runat="server">
    <title>生产日报查询</title>
</head>
<body>
    <form id="form1" runat="server">
    <div>
    <div align="center" class="STYLE1">
<br />
  <p style="font-weight: bold; font-size: 30px; color: black">
      生产日报查询</p><br />

</div>
<table width="800" border="0"  cellpadding="0"   cellspacing="1" bgcolor="#000000" align="center">
     <tr align="center" bgcolor="#add2da">
    <td   style="height: 28px">
        起始日期：</td>
    <td style="height: 28px">
        <asp:TextBox ID="qsrq" runat="server" Width="120px"></asp:TextBox>&nbsp;
    </td>
    <td style="height: 28px">
        结束日期：</td>
    <td style="height: 28px">
        <asp:TextBox ID="jsrq" runat="server" Width="120px"></asp:TextBox>&nbsp;
    </td>
    <td colspan="1" rowspan="2">&nbsp;&nbsp; &nbsp;
        <asp:Button ID="check" runat="server" Text="查询" Width="65" OnClick="check_Click" />    </td>
  </tr>
  <tr align="center" bgcolor="#add2da">
    <td   style="height: 28px">批次：</td>
    <td style="height: 28px">
        <asp:TextBox ID="pc" runat="server" Width="120px"></asp:TextBox>&nbsp;
    </td>
    <td style="height: 28px">型号：</td>
    <td style="height: 28px">
        <asp:TextBox ID="xh" runat="server" Width="120px"></asp:TextBox>&nbsp;
    </td>
  </tr>

</table>

    </div>
    </form>
</body>
</html>
